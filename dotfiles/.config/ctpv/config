set chafasixel

remove audio
remove video

preview audio audio/* {{
  ffmpegthumbnailer -i "$f" -s 0 -q 0 -o "$cache_f" 2>/dev/null
  convert_and_show_image audio_thumb
}}

preview video video/* {{
  ffmpegthumbnailer -i "$f" -s 0 -q 0 -t 10% -o "$cache_f" 2>/dev/null
  convert_and_show_image video
}}
